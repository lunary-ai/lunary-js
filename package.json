{
  "name": "lunary",
  "version": "0.6.4",
  "description": "Lunary is an open-source monitoring and analytics platform for AI apps.",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./openai": {
      "types": "./dist/openai.d.ts",
      "import": "./dist/openai.js",
      "require": "./dist/openai.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "import": "./dist/browser.js",
      "require": "./dist/browser.cjs"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js",
      "require": "./dist/react.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "openai": [
        "./dist/openai.d.ts"
      ],
      "types": [
        "./dist/types.d.ts"
      ],
      "browser": [
        "./dist/browser.d.ts"
      ],
      "react": [
        "./dist/react.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsup"
  },
  "author": "lunary",
  "license": "Apache-2.0",
  "dependencies": {
    "unctx": "^2.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.5.1",
    "@types/react": "^18.2.21",
    "bun-types": "^1.0.15",
    "dotenv": "^16.3.1",
    "langchain": "^0.0.167",
    "openai": "^4.20.1",
    "react": "^18.0.2",
    "stream-buffers": "^3.0.2",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "prettier": {
    "semi": false
  },
  "peerDependencies": {
    "openai": "^4.0.0",
    "react": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "openai": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  }
}
